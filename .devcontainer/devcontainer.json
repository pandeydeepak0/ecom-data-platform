{
  "name": "Ecom Data Platform Dev",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "docker-in-docker": "1"
  },
  "postCreateCommand": "./scripts/post_create.sh",
  "forwardPorts": [3000, 8080, 5432, 29092],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "ms-python.python",
        "njpwerner.autodocstring",
        "eamodio.gitlens"
      ]
    }
  }
}
